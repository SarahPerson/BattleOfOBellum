        if(curRow&gt;0)
            curRow--;
        global.roomTiles[curCol,curRow].playerTwoSelected=true;
    
        canMove=false;
        }
        if(alarm[0]==-1)
        {
           alarm[0]= room_speed*(0.1);
        }
    
    }
}


    


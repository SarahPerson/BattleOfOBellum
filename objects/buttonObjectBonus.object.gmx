            PlayerOne.lvPoints += 3;
            PlayerOne.selectedBonus = true;
            PlayerOne.inMenu = true;
            break;
        case "Place objective":
            global.setFlag = true;
            PlayerOne.selectedBonus = true;
            PlayerOne.inMenu = true;
            break;
        case "Place a wall":
            global.setWall = 5;
            PlayerOne.selectedBonus = true;
            PlayerOne.inMenu = true;

        <arguments>
          <argument>
            <kind>1</kind>
            <string>//what to do on click
if(currentIncrementTwo = action &amp;&amp; player==2&amp;&amp; PlayerTwo.inMenu!=true &amp;&amp; !PlayerTwo.isSelectingCharacterForBonus &amp;&amp; !PlayerTwo.isSelectingCharacterForBonus &amp;&amp; !PlayerTwo.selectedBonus ){
    switch(buttonText)
    {
    //do actual stuff here
        case "Heal entire team":
            //set the team to heal and call the alarm with the event
            GameMaster.teamToHeal = 1;
            if(GameMaster.alarm[3] == -1) {GameMaster.alarm[3] = room_speed*0.1;}
            
            PlayerTwo.selectedBonus = true;
            PlayerTwo.inMenu = true;
            break;
        case "Heal one character fully":
            //set alarm to set character to be selected boolean
            if(PlayerTwo.alarm[5] == -1) {PlayerTwo.alarm[5] = room_speed*0.1;}
            //Set initially highlighted tile
            GameMaster.TeamTwo[PlayerTwo.bonusRow, PlayerTwo.bonusRow].playerTwoHighlighted = true;
            PlayerTwo.bonusCanMove=false
            PlayerTwo.selectedBonus = true;
            
            //reset bonus coll and row
            PlayerTwo.bonusCol = 0;
            PlayerTwo.bonusRow = 0;
            break;
        case "Get extra level up points":
            //add points to total level up points
            PlayerTwo.lvPoints += 3;
            
            PlayerTwo.selectedBonus = true;
            PlayerTwo.inMenu = true;
            break;
        case "Place objective":
            global.setFlag = true;
            PlayerTwo.selectedBonus = true;
            PlayerTwo.inMenu = true;
            break;
        case "Place a wall":
            global.setWall = 5;
            PlayerTwo.selectedBonus = true;
            PlayerTwo.inMenu = true;
            break; 
        default:
            break;
    }
    
}
</string>

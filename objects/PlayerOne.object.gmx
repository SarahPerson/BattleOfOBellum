            <string>///Move Cursor
//Move highlighted tile to where current location of player
if(room==SplitScreen){
    if(keyboard_check(key_down))
    {
        if(canMove){
            global.gridTwo[curCol,curRow].playerOneSelected=false;
            if(curRow&lt;global.roomSize-1)
                curRow++;
                
            global.gridTwo[curCol,curRow].playerOneSelected=true;
        
            canMove=false;
        }    
            if(alarm[0]==-1)
            {
               alarm[0]= room_speed*(0.1);
            }
        
    }
    
    if(keyboard_check(key_right))
    {
        if(canMove){
            global.gridTwo[curCol,curRow].playerOneSelected=false;
            if(curCol&lt;global.roomSize-1)    
                curCol++;
            global.gridTwo[curCol,curRow].playerOneSelected=true;
        
            canMove=false;
        
        }    
            if(alarm[0]==-1)
            {
               alarm[0]= room_speed*(0.1);
            }
        
    
    }
    if(keyboard_check(key_left))
    {
        if(canMove){
            global.gridTwo[curCol,curRow].playerOneSelected=false;
            if (curCol&gt;0)
                curCol--;
            global.gridTwo[curCol,curRow].playerOneSelected=true;
        
            canMove=false;
        
        }    
            if(alarm[0]==-1)
            {
               alarm[0]= room_speed*(0.1);
            }
        
    }
    
    if(keyboard_check(key_up))
    {
    
        if(canMove){
            global.gridTwo[curCol,curRow].playerOneSelected=false;
            if(curRow&gt;0)
                curRow--;
            global.gridTwo[curCol,curRow].playerOneSelected=true;
        
            canMove=false;
        }    
            if(alarm[0]==-1)
            {
               alarm[0]= room_speed*(0.1);
            }
        
    
    }
}


    


if(currentIncrement = action){
    switch(currentIncrement)
    {
        case 0:
            room_goto(Drafting);
            break;
        case 1:
            if(background_visible[0]){
                background_visible[0]=false;
                background_visible[1]=true;
            }
            else if(background_visible[2])
            {
                background_visible[2]=false;
                background_visible[1]=true;
            }
            else{
                background_visible[1]=false;
                background_visible[2] = false;
                background_visible[0] = true;
            }
            break;     
        case 2:
            if(background_visible[0]){
                background_visible[0]=false;
                background_visible[2]=true;
            }
            else if(background_visible[1])
            {
                background_visible[1]=false;
                background_visible[2]=true;
            }
            else{
                background_visible[1]=false;
                background_visible[2] = false;
                background_visible[0]=true;
                
            }
            break;
        case 3:
            room_goto(SettingsScreen);
            break;
